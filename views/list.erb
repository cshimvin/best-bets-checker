<div class="row">
  <h1>Funnelback best bets checker</h1>
  <h2><%= title %></h2>
</div>

<div class="row">
  <p><%= filename %></p>
  <table>
  <tr><td>Search term</td><td>Title</td><td>Description</td><td>URL</td></tr>
  <% File.open(filename).each do |line| %>
    <tr><td><%= line.gsub("==","</td><td>") %></tr>
  <% end %>
  </table>
</div>